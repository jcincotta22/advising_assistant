<%= render 'layouts/errors' %>

<%= form_for [@advisee, @graduation_plan] do |f| %>
  <div class="row center">
    <div class="col s12 m6">
      <div class="input-field">
        <%= f.text_field :name %>
        <%= f.label :name %>
      </div>
    </div>
  </div>

  <%= f.submit 'Save Graduation Plan', class: 'btn standard' %>
<% end %>
