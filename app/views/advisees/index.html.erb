<div class="container">
</div>

<div class="row">
  <div class="col s12 m8">
    <div class="row">
      <div class="col s12 m8">
        <h1>Advisees</h1>
      </div>
      <div class="col s12 m4">
        <%= link_to new_advisee_path, class: 'btn add-advisee' do %>
           <i class="material-icons left">add</i>
          Add Advisee
        <% end %>
      </div>
    </div>

    <div class="row">
      <div class="col s12">
        <div id="advisee-search">
          <div class="advisees">
            <% @advisees.each do |advisee| %>
              <%= render partial: 'advisees/advisee', object: advisee %>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col s12 m4">
    <div id="upcoming-meetings">
    </div>
  </div>
</div>
